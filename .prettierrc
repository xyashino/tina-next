{
  "$schema": "http://json.schemastore.org/prettierrc",
  "semi": false,
  "arrowParens": "avoid",
  "trailingComma": "none",
  "singleQuote": true,
  "endOfLine": "lf",
  "bracketSpacing": true,
  "printWidth": 80,
  "useTabs": false,
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "plugins": ["prettier-plugin-tailwindcss"]
}
